<table class="table table-bordered table-striped table-dark">
    <thead class="thead">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nome Completo</th>
            <th scope="col">CPF</th>
            <th scope="col">Data de Nascimento</th>
            <th scope="col">Telefone</th>
            <th scope="col">Editar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in ViewBag.Usuarios)
        {
            <tr id="adminTr@(usuario.IdUsuario)" class="adminTr">
                <th scope="row">@usuario.IdUsuario</th>
                <td class="info-nome">@usuario.NomeCompleto</td>
                <td>@usuario.CPF</td>
                <td>@usuario.DataNascimento</td>
                <td>@usuario.Telefone</td>

                <td class="d-flex justify-content-center">
                    <button class="btn btn-info ml-3" onclick="Editar('#adminTr@(usuario.IdUsuario)')" data-toggle="modal" data-target=".bd-example-modal-lg"><i class="fas fa-pen"></i></button>
                </td>
            </tr>
        }
    </tbody>
</table>
